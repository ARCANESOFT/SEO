{
    "name":        "arcanesoft/seo",
    "description": "SEO Module for ARCANESOFT CMS.",
    "keywords":    [
        "arcanedev", "arcanesoft", "laravel", "seo"
    ],
    "homepage":    "https://github.com/ARCANESOFT/SEO",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                    ">=5.6.4",
        "arcanesoft/core":        "~1.4",
        "arcanedev/laravel-seo":  "~0.1",
        "arcanedev/spam-blocker": "~1.0"
    },
    "require-dev": {
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Seo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Seo\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
